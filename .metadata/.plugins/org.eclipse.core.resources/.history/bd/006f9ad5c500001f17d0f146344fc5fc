package com.stl.example;

public class Cake {
	
	public String flavour;
	public double fields;
	
	
	
	public Cake(String flavour, double fields) {
		super();
		this.flavour = flavour;
		this.fields = fields;
	}
	public String getFlavour() {
		return flavour;
	}
	public void setFlavour(String flavour) {
		this.flavour = flavour;
	}
	public double getFields() {
		return fields;
	}
	public void setFields(double fields) {
		this.fields = fields;
	}
	
	

}
