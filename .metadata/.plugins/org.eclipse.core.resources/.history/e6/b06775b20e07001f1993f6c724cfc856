package com.stl.example;

public class WeddingCake extends Cake {
	
	int tiers;

	public WeddingCake(String flavour, double fields) {
		super(flavour, fields);
		// TODO Auto-generated constructor stub
		this.flavour = "almond";
	}

	public int getTiers() {
		return tiers;
	}

	public void setTiers(int tiers) {
		this.tiers = tiers;
	}
	
	
	
	
	
	

}
